{
    "name": "web",
    "version": "0.1.0",
    "description": "",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run build-sass && npm run build-ts && npm run build-ts-client && npm run lint && npm run copy-static-assets",
        "serve": "node dist/server.js",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node,ClientNode\" -c \"yellow.bold,cyan.bold,green.bold,orange.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run watch-build-ts-client\" ",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "build-ts": "tsc --project tsconfig.server.json",
        "build-ts-client": "npx webpack",
        "watch-build-ts-client": "npx webpack --watch",
        "watch-ts": "tsc --project tsconfig.server.json -w",
        "build-sass": "node-sass src/server/public/css/main.scss dist/public/css/main.css",
        "watch-sass": "node-sass -w src/server/public/css/main.scss dist/public/css/main.css",
        "lint": "tsc --noEmit && eslint \"**/**/*.{js,ts}\" --quiet --fix",
        "copy-static-assets": "ts-node copyStaticAssets.ts",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js localhost 9229",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold,orange.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\" \"npm run watch-build-ts-client\""
    },
    "dependencies": {
        "@babel/runtime": "^7.11.2",
        "@types/react": "^16.9.35",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.5",
        "@types/react-transition-group": "^4.4.0",
        "async": "^3.1.0",
        "bcrypt-nodejs": "^0.0.3",
        "bluebird": "^3.5.5",
        "body-parser": "^1.19.0",
        "chokidar": "^3.4.2",
        "compression": "^1.7.4",
        "concurrently": "^5.3.0",
        "connect-mongo": "^3.0.0",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.16.2",
        "express-validator": "^6.2.0",
        "fbgraph": "^1.4.4",
        "lodash": "^4.17.15",
        "lusca": "^1.6.1",
        "mongoose": "^5.7.5",
        "nats": "^1.4.9",
        "nodemailer": "^6.3.0",
        "passport": "^0.4.0",
        "passport-facebook": "^3.0.0",
        "passport-local": "^1.0.0",
        "pug": "^2.0.4",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-router-dom": "^5.2.0",
        "ts-nats": "^1.2.15",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/async": "^3.0.2",
        "@types/bcrypt-nodejs": "^0.0.30",
